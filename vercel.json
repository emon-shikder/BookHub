{
    "builds": [
      {
        "src": "BookHub/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12.4"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "BookHub/wsgi.py"
      }
    ],
    "rewrites": [
    { "source": "/static/(.*)", "destination": "/static/$1" },
    { "source": "/media/(.*)", "destination": "/media/$1" }
    ]
  }
  